{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "npx nuxi typecheck",
    "lint": "eslint .",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "check-branch-name": "bash ./.husky/validate_current_branch.sh",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "format:ci": "prettier --list-different .",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "slicemachine": "start-slicemachine"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,vue}": [
      "eslint . --fix",
      "eslint ."
    ],
    "./src/**/*.{css,scss,vue}": [
      "stylelint ."
    ],
    "**/*": [
      "prettier . --write"
    ]
  },
  "dependencies": {
    "@nuxtjs/i18n": "9.1.1",
    "@nuxtjs/robots": "5.1.0",
    "@nuxtjs/sitemap": "7.0.1",
    "@nuxtjs/supabase": "1.4.5",
    "@unlok-co/nuxt-stripe": "4.0.1",
    "@vueuse/core": "^12.2.0",
    "@vueuse/nuxt": "^12.2.0",
    "nuxt": "^3.14.1592",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@histoire/plugin-nuxt": "^0.17.17",
    "@histoire/plugin-vue": "^0.17.17",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@nuxt/eslint": "^0.7.4",
    "@nuxtjs/prismic": "3.4.6",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@slicemachine/adapter-nuxt": "^0.3.61",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "0.5.9",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "eslint": "^9.17.0",
    "histoire": "^0.17.17",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "3.4.2",
    "prettier-plugin-sort-json": "4.0.0",
    "slice-machine-ui": "^2.11.1",
    "stylelint": "^16.12.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.2",
    "vite-plugin-eslint2": "^5.0.3",
    "vue-tsc": "^2.1.10"
  }
}
